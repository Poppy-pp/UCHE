// pl
.toolbar .el-form-item {
    margin-bottom: 1% !important;
}
.stepState{
  margin-bottom: 1%; 
}
.card{
  padding: 1%;
  text-align:center;
}
.time {
    color: #999;
    line-height: 30px;
  }
  .button {
    padding: 0;
    float: right;
  }
  .image {
    width: 100%;
    display: block;
  }
  .clearfix:before,
  .clearfix:after {
      display: table;
      content: "";
  }
  .clearfix:after {
      clear: both
  }
  .el-input-group__append, .el-input-group__prepend{
    padding: 0 10px !important;
  }
  .asscard .el-card{
    margin-bottom: 10%;
  }
  .asscard .addBtn{
    float: right;
    margin: 2% 0 2% 1%;
  }
  .assnew .el-autocomplete{
    width: 100%;
  }
  .selectcol .ivu-select-dropdown{
    padding: 5px 0 0 0;
  }




